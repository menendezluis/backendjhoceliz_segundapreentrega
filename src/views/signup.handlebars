<div class="login-container">
    <form id="signup-form" class="signup-form">
        <h2>Crea tu usuario</h2>
        <div class="form-group">

            <label for="first_name">Nombre:</label>
            <input class="input-login" type="text" id="first_name" name="first_name" required />
        </div>
        <div class="form-group">

            <label class="label-login" for="last_name">Apellido:</label>
            <input class="input-login" type="text" id="last_name" name="last_name" required />
        </div>
        <div class="form-group">

            <label class="label-login" for="age">Edad:</label>
            <input class="input-login" type="number" id="age" name="age" required />
        </div>
        <div class="form-group">

            <label class="label-login" for="username">Email:</label>
            <input class="input-login" type="email" id="username" name="username" required />
        </div>
        <div class="form-group">

            <label class="label-login" for="password">Contrase√±a:</label>
            <input class="input-login" type="password" id="password" name="password" required />

        </div>
        <button id="signup" type="submit">Ingresar</button>
    </form>
</div>
<script src="/js/singup.js"></script>